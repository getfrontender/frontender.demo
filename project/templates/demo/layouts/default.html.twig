{% extends 'demo/layouts/base.html.twig' %}

{% block title %}
    <title>{{ page_config.seo.controls.title.value|t }}</title>
{% endblock %}

{% block meta %}
    <meta name="robots" content="{{ page_config.seo.controls.meta_robots.value|default('index,follow')|t }}">
    {% if page_config.seo.controls.meta_description.value|t is not empty %}
        <meta name="description" content="{{ page_config.seo.controls.meta_description.value|t }}">
    {% endif %}
    {% if page_config.seo.controls.meta_keywords.value|t is not empty %}
        <meta name="keywords" content="{{ page_config.seo.controls.meta_keywords.value|t }}">
    {% endif %}
{% endblock %}

{% block stylesheets %}
    <link href="{{ hashedPath('/assets/styles/main.css') }}" rel="stylesheet"></link>
{% endblock %}

{% block scripts %}
{{ '/assets/scripts/jquery.min.js'|addScript('beforeBodyEnd') }}
{{ '/assets/scripts/jquery.scrollex.min.js'|addScript('beforeBodyEnd') }}
{{ '/assets/scripts/skel.min.js'|addScript('beforeBodyEnd') }}
{{ '/assets/scripts/util.js'|addScript('beforeBodyEnd') }}
{{ '/assets/scripts/main.js'|addScript('beforeBodyEnd') }}
{% endblock %}
