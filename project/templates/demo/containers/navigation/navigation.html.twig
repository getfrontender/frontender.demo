{% import "macros/template.html.twig" as helper %}

<{{ config.container_tag|default('nav') }} id="menu" class="{{[config.container_class]|join('  |  ') }}" {{ helper.container_id_attribute(frontender, 'frontender')|raw }}>
    <ul class="links">
        {% for link in content.links %}
            <li><a href="{{ route({'page': link.page }) }}">{{ link.label|t(locales) }}</a></li>
        {% endfor %}
    </ul>
</{{ config.container_tag|default('nav') }}>
