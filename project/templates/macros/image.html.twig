{% macro crop(object, config = {}, class = "") %}

    {% set config = {w: 640, h: 480, fit: "crop", crop: "faces", q: 80}|merge(config)|key_value_join("=") %}
    {# {% set config = image.get_config %} #}

    <pre>{{ [object.urls.raw, config]|join("&") }}</pre>

{% endmacro %}